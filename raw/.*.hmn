html {
  background: linen;
}

div.main {
  font-family: sans-serif;
}

div.note {
  background: lightskyblue;
}

div.warning {
  background: coral;
}


span.block-title {
  display: block;
  padding: 3px;
  background: black;
  font-size: large;
  font-weight: bold;
}

span.note-title {
  color: lightskyblue;
}
span.warning-title {
  color: coral;
}

div.H {
  margin: 10px;
  border: solid black 1px;
}

div.H pre.H-code {
  margin: 0;
  background: yellow;
}

div.H div {
  margin: 10px;
}

div.H span.H-title {
  display: inline-block;
  padding: 3px;
  padding-right: 6px;
  width: 4em;
  text-align: right;
  font-family: monospace;
  background: black;
  font-weight: bold;
  color: yellow;
}
